@model Vidly.Models.Customer

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.MembershipTypeName</li>
    @if(@Model.Birthday != null)
    {
        <li>Birthday: @Model.Birthday</li>
    }
    
</ul>

